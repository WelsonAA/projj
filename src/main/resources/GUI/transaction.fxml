<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="539.0" prefWidth="514.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.HelloController">
   <children>
      <ImageView fitHeight="544.0" fitWidth="514.0" pickOnBounds="true">
         <image>
            <Image url="@../background.jpg" />
         </image>
      </ImageView>

      <Button fx:id="withdraw" layoutX="117.0" layoutY="70.0" mnemonicParsing="false" onAction="#gowithdraw" prefHeight="77.0" prefWidth="257.0" text="Withdraw">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="deposit" layoutX="117.0" layoutY="175.0" mnemonicParsing="false" onAction="#godeposit" prefHeight="77.0" prefWidth="257.0" text="Deposit">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="transfer" layoutX="117.0" layoutY="285.0" mnemonicParsing="false" onAction="#gotransfer" prefHeight="77.0" prefWidth="257.0" text="Transfer">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="homebalance" layoutX="118.0" layoutY="399.0" mnemonicParsing="false" onAction="#gohomefrombalance" prefHeight="77.0" prefWidth="257.0" text="Home">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
